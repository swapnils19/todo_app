<div class="clear center">
  <h3 class="center">Projects</h3>
</div>
<div class="clear">
<table class="table table-striped">
  <thead>
    <tr>
      <th>Project name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%= project.project_name %></td>
        <td><%= link_to 'Show', project %></td>
        <td><%= link_to 'Edit', edit_project_path(project) %></td>
        <td><%= link_to 'Delete Project', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>

<div class="clear">
<%= link_to 'New Project', new_project_path, class: 'btn btn-default' %>
<%= link_to 'New Todo', new_todo_path, class: 'btn btn-default' %>
<%= link_to 'Developers Status', users_dashboard_admin_dashboard_path, class: 'btn btn-default' %>
</div>
<br>